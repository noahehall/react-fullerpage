{
  "name": "react-fullerpage",
  "version": "0.1.1",
  "description": "fork of react-fullpage, for a fuller page",
  "author": "forked from thierryc",
  "license": "MIT",
  "main": "dist/index.js",
  "module": "src/index.tsx",
  "scripts": {
    "barrels": "barrelsby --config ./barrels.json",
    "build": "rm -rf ./dist/* && bun build src/index.tsx --outdir ./dist",
    "lint": "bunx eslint src -- --fix",
    "pretty": "bunx prettier --write 'src/**/*.(ts|tsx)'",
    "upgrade:deps": "bunx ncu -u && bun install"
  },
  "devDependencies": {
    "@types/react": "18.2.6",
    "@types/react-dom": "18.2.4",
    "@types/react-router-dom": "5.3.3",
    "@typescript-eslint/eslint-plugin": "5.59.7",
    "@typescript-eslint/parser": "5.59.7",
    "barrels": "^1.6.6",
    "bun-types": "0.6.3",
    "eslint": "8.41.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.32.2",
    "gh-pages": "^5.0.0",
    "npm-check-updates": "^16.10.12",
    "prettier": "^2.8.8"
  },
  "files": [
    "dist"
  ]
}
